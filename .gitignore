# Create .gitignore with all the necessary exclusions
cat > .gitignore << 'EOF'
# Python
__pycache__/
*.py[cod]
*.so
.Python
build/
dist/
*.egg-info/
*.egg

# Virtual environments
venv/
env/
.venv/

# Environment variables
.env
.env.local

# Database files
*.db
*.sqlite3
test.db

# Node.js
node_modules/
.npm-debug.log*
yarn-debug.log*
yarn-error.log*
.next/

# Frontend build
frontend/out/
frontend/.next/

# Logs
*.log
logs/

# IDE
.vscode/
.idea/
*.swp
*.swo

# OS
.DS_Store
Thumbs.db

# Temporary files
*.tmp
*.temp
*.pdf

# Large binary files
*.zip
*.tar.gz
EOF